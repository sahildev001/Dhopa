<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dhopa">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon_dhopa"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Dhopa">
        <activity android:name=".VarificationDoneActivity"></activity>
        <activity android:name=".BottomNavActivity" />
        <activity android:name=".VerificationActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".LogIn" />
        <activity android:name=".NotificationActivity"/>
        <activity android:name=".SchedulePickupActivity"/>
        <activity android:name=".SchedulePickupActivity2"/>
        <activity android:name=".SchedulePickupActivity3"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>